<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="SocietyView.css" />
	<g:HTMLPanel>
		<div>
			<g:Label ui:field="close" addStyleNames="{style.popupCloseButton}" />
			<div class="{style.societyTitleWrapper}">
				<div class="{style.selectionDisplay}">
					<g:HTMLPanel addStyleNames="{style.societyHeader}">
						<h1><g:Label ui:field="societyName" /></h1>
						<h3>Nombre de fantasía: </h3>
						<h3><strong><g:Label styleName="{style.societySubTitle}" ui:field="societyFantasyName" /></strong></h3>
					</g:HTMLPanel>
				</div>
			</div>
			<table class="{style.societyTable}">
				<tr>
					<th scope="row"><div>RUT Sociedad:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societyUid" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Fecha de constitución:</div></th>
					<td class="{style.altTd}"><g:Label ui:field="societyCreationDate" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Último capital declarado:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societyCurrentStock" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Situación tributaria:</div></th>
					<td class="{style.altTd}"><g:Label ui:field="societyStatus" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Objeto de la sociedad:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societySubject" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Tipo de sociedad:</div></th>
					<td class="{style.altTd}"><g:Label ui:field="societyType" /></td>
				</tr>
				<tr>
					<th scope="row"><div>Socios actuales:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societyMembers" /></td>
				</tr>
			</table>
			<div class="{style.societyTitleWrapper}">
				<div class="{style.selectionDisplay}">
					<h1>Otros datos de la sociedad</h1>
				</div>
			</div>
			<table class="{style.societyTable}">
				<tr>
					<th><div>Capital inicial:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societyInitialStock" /></td>
				</tr>
				<tr>
					<th><div>Capital aportado por el parlamentario:</div></th>
					<td class="{style.altTd}"><g:Label ui:field="parlamentarianStock" /></td>
				</tr>
				<tr>
					<th><div>Domicilio:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="societyAddress" /></td>
				</tr>
				<tr>
					<th><div>Fecha de publicación:</div></th>
					<td class="{style.altTd}"><g:Label ui:field="societyPublishDate" /></td>
				</tr>
				<tr>
					<th><div>Notaría:</div></th>
					<td class="{style.normalTd}"><g:Label ui:field="notaryName" /></td>
				</tr>
			</table>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>