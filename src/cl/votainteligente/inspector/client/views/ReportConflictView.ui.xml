<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="ReportConflictView.css" />
	<g:HTMLPanel>
		<h2>Reportar posible conflicto</h2>
		<g:HTMLPanel>
			Seleccione un parlamentario: <g:ListBox ui:field="parlamentarianList"></g:ListBox>
			Reporte: <g:TextArea ui:field="report" />
			<g:Button ui:field="submit">Enviar</g:Button>
			<g:Button ui:field="clear">Limpiar</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>