<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="ReportConflictView.css" />
	<g:HTMLPanel addStyleNames="{style.container}">
		<h2>Reportar posible conflicto</h2>
		<g:HTMLPanel>
			<p>Seleccione un parlamentario:</p>
			<g:ListBox ui:field="parlamentarianList" addStyleNames="{style.listBoxReport}" ></g:ListBox>
			<p>Reporte:</p>
			<g:TextArea addStyleNames="{style.textAreaReport}" ui:field="report" />
			<br />
			<g:Button ui:field="submit">Enviar</g:Button>
			<g:Button ui:field="clear">Limpiar</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>