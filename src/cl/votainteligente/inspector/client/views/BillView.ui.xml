<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="BillView.css" />
	<g:HTMLPanel>
		<g:HTMLPanel ui:field="billPanel">
			Boletín: <g:Label ui:field="billBulletinNumber"/>
			Título: <g:Label ui:field="billTitle"/>
			Fecha Ingreso: <g:Label ui:field="billEntryDate"/>
			Moción: <g:Label ui:field="billInitiativeType"/>
			Tipo: <g:Label ui:field="billType"/>
			Cámara: <g:Label ui:field="billOriginChamber"/>
			Urgencia: <g:Label ui:field="billUrgency"/>
			Estado: <g:Label ui:field="billStage"/>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="parlamentarianPanel">
			Parlamentarios en conflicto:
			<c:CellTable ui:field="parlamentarianTable"/>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="societyPanel">
			Sociedades en conflicto:
			<c:CellTable ui:field="societyTable"/>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>