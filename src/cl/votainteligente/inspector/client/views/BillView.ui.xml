<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="BillView.css" />
	<g:HTMLPanel>
		<h2>Proyecto de Ley:</h2>
		<g:HTMLPanel ui:field="billPanel">
			<g:Label addStyleNames="billTitle" ui:field="billTitle"/>
			<div class="{style.billdescription}">
				<g:Label ui:field="billDescription"></g:Label>
			</div>
			<div class="{style.billData}">
				N° Boletín: <g:Label ui:field="billBulletinNumber"/>
				Fecha de ingreso: <g:Label ui:field="billEntryDate"/>
				Iniciativa: <g:Label ui:field="billInitiativeType"/>
				Tipo: <g:Label ui:field="billType"/>
				Cámara de origen: <g:Label ui:field="billOriginChamber"/>
				Urgencia: <g:Label ui:field="billUrgency"/>
				Estado: <g:Label ui:field="billStage"/>
			</div>
		</g:HTMLPanel>
		<div class="{style.conflictWrapper}">
			<g:HTMLPanel>
				<g:Image ui:field="parlamentarianImage"/>
				<g:Label ui:field="parlamentarianDisplay"></g:Label>
				<g:Label ui:field="parlamentarianProfileLink">ver su perfil de intereses</g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="societyPanel">
				Sociedades en conflicto:
				<g:HTMLPanel ui:field="societyTableContainer"></g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="parlamentarianPanel">
				Parlamentarios en conflicto:
				<g:HTMLPanel ui:field="parlamentarianTableContainer"></g:HTMLPanel>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
