<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="PermalinkView.css" />
	<g:FocusPanel ui:field="permalinkPanel">
		<g:HTMLPanel>
			<g:Label ui:field="close" addStyleNames="{style.popupCloseButton}" />
			<g:TextBox ui:field="permalink" addStyleNames="{style.popupWidthBox}"/>
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder>