<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="PopupView.css" />
	<g:HTMLPanel visible="false">
		<g:PopupPanel ui:field="popup" modal="true" glassEnabled="true" autoHideOnHistoryEventsEnabled="true">
			<g:HTMLPanel>
				<g:FlowPanel ui:field="layout"/>
				<g:Button ui:field="close">Cerrar</g:Button>
			</g:HTMLPanel>
		</g:PopupPanel>
	</g:HTMLPanel>
</ui:UiBinder> 