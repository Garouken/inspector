<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="HomeView.css" />
	<g:HTMLPanel>
		<g:HTMLPanel ui:field="parlamentarianPanel">
			<g:Label ui:field="parlamentarianLabel"/>
			<g:TextBox ui:field="parlamentarianSearch"/>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="categoryPanel">
			<g:Label ui:field="categoryLabel"/>
			<g:TextBox ui:field="categorySearch"/>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<g:Label ui:field="parlamentarianDisplay"></g:Label> + <g:Label ui:field="categoryDisplay"></g:Label>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="billPanel">
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>